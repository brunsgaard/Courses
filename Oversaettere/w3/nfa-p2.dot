digraph {
  rankdir = LR;
  margin = 0;
  node [shape=circle];

  start [label="", shape=none, width=0, height=0];
  E  [label="E"];
  F  [label="F"];
  G  [label="G"];
  H  [label="H"];
  I  [label="I", shape=doublecircle];

  start-> E;
  E -> F [label="a"];
  F -> G [label="("];
  G -> H [label="E"];
  H -> I [label=")"];
}
